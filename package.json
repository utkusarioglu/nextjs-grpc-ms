{
  "name": "ms.nextjs-grpc.projects.utkusarioglu.com",
  "version": "1.0.0",
  "description": "\"microservice\" container for the nextjs gprc workshop",
  "main": "dist/index.js",
  "author": "Utku Sarioglu <utkusarioglu@hotmail.com>",
  "license": "MIT",
  "private": true,
  "scripts": {
    "dev": "scripts/dev.sh",
    "dev:no-telemetry": "scripts/dev-no-telemetry.sh",
    "start": "scripts/start.sh",
    "debug": "scripts/debug.sh",
    "remove-build": "rm -rf $(jq -r '.compilerOptions.outDir' tsconfig.json)",
    "remove-declarations": "rm -rf types",
    "remove-artifacts": "yarn remove-build && yarn remove-declarations",
    "build": "yarn remove-artifacts && yarn tsc -p tsconfig.build.json",
    "generate:buf": "yarn buf generate"
  },
  "dependencies": {
    "@grpc/grpc-js": "^1.8.14",
    "@opentelemetry/api": "^1.4.1",
    "@opentelemetry/auto-instrumentations-node": "^0.36.5",
    "@opentelemetry/exporter-prometheus": "^0.38.0",
    "@opentelemetry/exporter-trace-otlp-grpc": "^0.38.0",
    "@opentelemetry/instrumentation-winston": "^0.31.1",
    "@opentelemetry/sdk-metrics-base": "^0.31.0",
    "@opentelemetry/sdk-node": "^0.38.0",
    "@opentelemetry/semantic-conventions": "^1.12.0",
    "@types/nconf": "^0.10.3",
    "async": "^3.2.4",
    "dotenv": "^16.0.3",
    "knex": "^2.4.2",
    "mock-knex": "^0.4.12",
    "nconf": "^0.12.0",
    "nconf-yaml": "^1.0.2",
    "pg": "^8.10.0",
    "pg-query-stream": "^4.4.0",
    "tsconfig-paths": "^4.2.0",
    "winston": "^3.8.2",
    "yaml": "^2.2.1"
  },
  "devDependencies": {
    "@bufbuild/buf": "^1.23.1",
    "@types/config": "^3.3.0",
    "@types/mock-knex": "^0.4.5",
    "@types/node": "^18.15.11",
    "@types/pg": "^8.6.6",
    "@types/yaml": "^1.9.7",
    "grpc-tools": "^1.12.4",
    "jest": "^29.5.0",
    "knex-mock-client": "^2.0.0",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.0.4",
    "@protobuf-ts/plugin": "^2.9.0"
  }
}
